#include <bits/stdc++.h>

using namespace std;

int main()
{

      vector<int> v={2,3,4,5};




         for(int i:v)
         {
             cout<<i<<" ";
                if(i%2==0)
                {
                    cout<<"this number is even"<<endl;
                }
                else
                {

                    cout<<"This numbr is odd"<<endl;
                }
         }




    return 0;
}
