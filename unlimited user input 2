#include <bits/stdc++.h>

using namespace std;

int main()
{

      vector<int> v;
      int n;
       while(cin>>n)
       {
           if(n==0)
           {
               break;
           }
           else
           {

               v.push_back(n);
           }

       }
       for(int i:v)
       {
           cout<<i<<endl;
       }








    return 0;
}
